{
  "expo": {
    "name": "Aziel_Mobile",
    "slug": "Aziel_Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/aziel_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "notification":{
      "icon": "./assets/images/aziel_icon.png"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/aziel_icon.png",
          "color": "#ff0000"
        }
      ]
    ]
  }
}
